<nav class="noo-md-8 noo-main-menu">
    <ul id="menu-menu-primary" class="nav-collapse navbar-nav">
        {% for element in elements %}
            <li id="menu-item-{{ element.id }}" class="noo-menu  menu-item menu-item-type-post_type menu-item-object-page menu-item-home current-menu-ancestor current-menu-parent current_page_parent current_page_ancestor menu-item-has-children ">
                {% if element.link %}
                    <a href="{{ url(element.link) }}">{{ element.label }}</a>
                {% else %}
                    <a href="#">{{ element.label }}</a>
                {% endif %}
                {% if element.activeChildren|length > 0 %}
                    <ul class="sub-menu">
                        {% for child in element.activeChildren %}
                            <li class="noo-menu menu-item menu-item-type-post_type_archive menu-item-object-noo_property ">
                                {% if child.link %}
                                    <a href="{{ url(child.link) }}">{{ child.label }}</a>
                                {% else %}
                                    <a href="{{ child.url }}">{{ child.label }}</a>
                                {% endif %}
                            </li>
                        {% endfor %}
                    </ul>
                {% endif %}
            </li>
        {% endfor %}
        <li class="magic-line hide-line" style="width: 78px; left: 0px;"></li>
    </ul>
</nav>
